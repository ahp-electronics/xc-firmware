<!ATTLIST	DebugMode
			value	(FALSE | TRUE)	#REQUIRED	>

<!ELEMENT	MCtrl	EMPTY >
<!ATTLIST 	MCtrl
		setting	CDATA #REQUIRED>
		
<!ELEMENT NVSector (SectorIndex, SectorName?, SectorFile?, SectorOper?, LocalLockPolicy?)	>
<!ATTLIST	NVSector	
		selected CDATA 		#REQUIRED>

<!ELEMENT	SectorIndex	(	#PCDATA	) >
<!ELEMENT	SectorName	(	#PCDATA	) >
