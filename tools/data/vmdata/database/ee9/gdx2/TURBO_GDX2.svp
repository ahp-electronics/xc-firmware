<Script>
<![CDATA[
function INIT {
	HDR 0;
	HIR 0;
	TDR 0;
	TIR 0;
	ENDDR IDLE;
	ENDIR IDLE;
	FREQUENCY 1E6 HZ;
	STATE IDLE;
	SYNC;
}
function READ_ID {
	SIR Instruction_Length 	TDI (IDCODE);
	SDR_VERIFY	ID_Length	TDI (#1^ID_Length)
							TDO	(IDTDI[1])
							MASK (DEVICE_ID_MASK);
	SYNC;
}
function ISC_READ_ID {
	SIR Instruction_Length 	TDI (IDCODE);
	SDR_VERIFY	ID_Length	TDI (#1^ID_Length)
							TDO	(IDTDI[1])
							MASK (DEVICE_ID_MASK);
	SYNC;
}
function PRELOAD {				
   // Shift in Preload instruction
   SIR Instruction_Length TDI(BPRELOAD);
   SDR BSCAN_Length TDI(Bscan[1]);
   SYNC;
}
function INI_PRELOAD {				
   // Shift in Preload instruction
   SIR Instruction_Length TDI(BPRELOAD);
   SDR BSCAN_Length TDI(#0^BSCAN_Length);
   SYNC;
}
function SAMPLE {				
   // Shift in Preload instruction
   SIR Instruction_Length TDI(BPRELOAD);
   SYNC;
   STATE DRPAUSE;
   STATE IDLE;
   SYNC;
}
function INI_SAMPLE {				
   // Shift in Preload instruction
   SIR Instruction_Length TDI(BPRELOAD);
   SYNC;
   STATE DRPAUSE;
   STATE IDLE;
   SYNC;
}

function DYNAMIC {	
   // Shift in Preload instruction
   ENDDR DRPAUSE;
   SIR Instruction_Length TDI(BPRELOAD);
   SDR BSCAN_Length TDI(Bscan[1])
   					DMASK(BscanMask[1]);
   ENDDR IDLE;
   STATE IDLE;
   SYNC;
}

function ENABLE {
	SYNC;
	SIR Instruction_Length TDI (ISC_ENABLE);
	RUN_TEST	TCK 3
				DELAY 20;
	SYNC;
}
function DISABLE {
	SIR	Instruction_Length 	TDI (ISC_DISABLE);
	RUN_TEST	TCK 3
				DELAY 200;
	SYNC;
}
function ERASE {
	SYNC;
	// Shift in Erase All instruction
	SIR Instruction_Length TDI (ISC_ERASE);
	RUN_TEST 	TCK 3 DELAY PWE;
	
	// Spinti 8/30/04 Added discharge
	SIR Instruction_Length TDI (ISC_DISCHARGE);
	RUN_TEST	TCK 3 DELAY 10;
	SYNC;
}
function PREERASE {
	// Shift in enable manufacuring mode
	SIR Instruction_Length TDI (LSCC_MANUFACTURING_SHIFT);
	SDR 40 TDI(#0000000100000000000000000000000001001101);
	// Shift in Enable instruction
	SIR Instruction_Length TDI (ISC_ENABLE);
	RUN_TEST	TCK 3 DELAY 20;
	
	// Shift in Erase All instruction
	SIR Instruction_Length TDI (ISC_ERASE);
	RUN_TEST	TCK 3 DELAY PWE;

	SIR Instruction_Length TDI (ISC_DISCHARGE);
	RUN_TEST	TCK 3 DELAY PWE;

	// Shift in Program DONE bit instruction 
	SIR	Instruction_Length	TDI (ISC_PROGRAM_DONE);
	// Goto Run Test Idle 
	RUN_TEST	TCK 3 DELAY PWP;


	// Shift in Program disable intruction
	SIR	Instruction_Length	TDI (ISC_DISABLE);
	RUN_TEST	TCK 3 DELAY 200;

	// Shift in Program disable intruction
	SIR	Instruction_Length	TDI (BYPASS);

	// Disable manufacturing mode
	SIR Instruction_Length TDI (LSCC_MANUFACTURING_SHIFT);
	SDR 40 TDI(#0^40);

	// Shift in preload intruction
	SIR Instruction_Length TDI(BPRELOAD);
   	SDR BSCAN_Length TDI(#0^BSCAN_Length);
   	//Shift in Intest instruction
   	SIR Instruction_Length TDI(INTEST);
	SYNC;
}
function FULL_ADDRESS_PROGRAM {
	FuseArray_TDI.row = 1;
	Address.row = 1;
	
	SIR 	Instruction_Length TDI (BYPASS);
	SDR	1	TDI (#0);
	SYNC;
	
	SIR Instruction_Length	TDI (ISC_ADDRESS_INIT);
	SDR	1	TDI (#0);
	
	
	// Program the rest of the device
	repeat Address_Length -1 {
		SIR Instruction_Length 	TDI (ISC_READ_INCR);	
		SDR	Row_Width	TDI	(FuseArray_TDI);
		SIR Instruction_Length	TDI (ISC_PROGRAM_INCR);
		RUN_TEST	TCK 	3
					DELAY 	PWP;
		SYNC;
		SIR Instruction_Length	TDI	(BYPASS);
		SDR	1	TDI	(#0);
	} // repeat
	Address[1] = #1@(#0^(Address_Length-1));
	SIR Instruction_Length 	TDI (ISC_READ_INCR);	
	SDR	Row_Width	TDI	(FuseArray_TDI);
	SIR Instruction_Length	TDI (ISC_PROGRAM_INCR);
	RUN_TEST	TCK 	3
				DELAY 	PWP;	
	SYNC;
}
function PROGRAM {
	FuseArray_TDI.row = 1;
	Address.row = 1;
	
	SIR 	Instruction_Length TDI (BYPASS);
	SDR	Address_Length	TDI (#0);
	SYNC;
	
	SIR Instruction_Length	TDI (ISC_ADDRESS_INIT);
	SDR	1	TDI (#0);

	repeat Address_Length -1 {
		SIR Instruction_Length 	TDI (ISC_READ_INCR);	
		SDR	Row_Width	TDI	(FuseArray_TDI);
		SIR Instruction_Length	TDI (ISC_PROGRAM_INCR);
		RUN_TEST	TCK 	3
					DELAY 	PWP;
		SYNC;
		SIR Instruction_Length	TDI	(BYPASS);
		ESDR	1	TDI (#0);
	} // repeat
	Address[1] = #1@(#0^(Address_Length-1));	
	SIR Instruction_Length 	TDI (ISC_READ_INCR);	
	SDR	Row_Width	TDI	(FuseArray_TDI);
	SIR Instruction_Length	TDI (ISC_PROGRAM_INCR);
	RUN_TEST	TCK 	3
				DELAY 	PWP;
	SYNC;
}
function PREPROGRAM {
	FuseArray_MASK.row = 1;
	
	// Shift in enable manufacuring mode
	SIR Instruction_Length TDI (LSCC_MANUFACTURING_SHIFT);
	SDR 40 TDI(#0000000100000000000000000000000001001101);
	// Shift in Enable instruction
	SIR Instruction_Length TDI (ISC_ENABLE);
	RUN_TEST	TCK 3 DELAY 20;
	
	// Shift in Erase All instruction
	SIR Instruction_Length TDI (ISC_ERASE);
	RUN_TEST	TCK 3 DELAY PWE;

	SIR Instruction_Length TDI (ISC_DISCHARGE);
	RUN_TEST	TCK 3 DELAY PWE;
	
	// Shift in Address init instruction
	SIR Instruction_Length	TDI (ISC_ADDRESS_INIT);

	STATE IDLE;

	// Load in program instruction with auto increment
      SIR Instruction_Length	TDI (ISC_PROGRAM_INCR);
	
	// Program the rest of the device
	repeat Address_Length {
	// Shift in row data 
	SDR	Row_Width	TDI	(FuseArray_MASK);
	RUN_TEST	TCK	3
			DELAY	PWP;
	} // repeat

	// Shift in Program DONE bit instruction 
	SIR	Instruction_Length	TDI (ISC_PROGRAM_DONE);
	// Goto Run Test Idle 
	RUN_TEST	TCK 3 DELAY PWP;

	// Shift in Program disable intruction
	SIR	Instruction_Length	TDI (ISC_DISABLE);
	RUN_TEST	TCK 3 DELAY 200;

	// Shift in Program disable intruction
	SIR	Instruction_Length	TDI (BYPASS);

	// Disable manufacturing mode
	SIR Instruction_Length TDI (LSCC_MANUFACTURING_SHIFT);
	SDR 40 TDI(#0^40);
	
	// Shift in preload intruction
	SIR Instruction_Length TDI(BPRELOAD);
   	SDR BSCAN_Length TDI(#0^BSCAN_Length);
   	//Shift in Intest instruction
   	SIR Instruction_Length TDI(INTEST);
 }
 
function FULL_ADDRESS_VERIFY {
	FuseArray_TDI.row = 1;
	Address.row = 1;	

	SIR 	Instruction_Length TDI (BYPASS);
	SDR	1	TDI (#0);
	SYNC;
	
	SIR Instruction_Length TDI (ISC_ADDRESS_SHIFT);
	SDR	Address_Length TDI (Address[1]);
	
	// Verify the device 
	repeat Address_Length -1 {
		SIR Instruction_Length TDI (ISC_READ_INCR);
		RUN_TEST	TCK 	3
					DELAY 	PWV;
		SIR Instruction_Length	TDI	(ISC_READ_INCR);
		STATE IDLE;
		SDR_VERIFY	Row_Width	TDI	(#1^Row_Width)
								TDO	(FuseArray_TDI)
								MASK(#1^Row_Width);
		SYNC;
		SIR Instruction_Length	TDI	(BYPASS);
		SDR	1	TDI	(#0);
	}
	Address[1] = #1@(#0^(Address_Length-1));
	SIR Instruction_Length TDI (ISC_READ_INCR);
	RUN_TEST	TCK 	3
				DELAY 	PWV;		
	SIR Instruction_Length 	TDI (ISC_READ_INCR);
	STATE IDLE;
	SDR_VERIFY	Row_Width	TDI	(#1^Row_Width)
							TDO	(FuseArray_TDI)
							MASK(#1^Row_Width);
	
	SYNC;
}
