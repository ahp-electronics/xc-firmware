	    SIR Instruction_Length	TDI	(LSCC_VERIFY_TAG);
		RUN_TEST	IDLE TCK 3 DELAY 	PWV;
		// Shift Out TAG Memory Data;
	    SDR_VERIFY	Arch_Length	TDI	(#0^Arch_Length)
								TDO	(ArchArray_TDI[1]);
		SYNC;								
	}
	SYNC;
}

