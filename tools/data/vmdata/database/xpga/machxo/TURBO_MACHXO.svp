function ATE_PROGRAM_STATUS {
	FuseArray_TDI.row = 1;
	// Shift in LSCC RESET ADDRESS(0x21) instruction
    SIR Instruction_Length	TDI (LSCC_RESET_ADDRESS);
	RUN_TEST	IDLE TCK 5 DELAY 	PWP;
