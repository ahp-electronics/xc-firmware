	INT32 Addr = 0;
    $ Initializing...
	Call INITIAL;	
    
    $ IDCode Checking...
	Call CHECK_ID;
		
    $ Erasing...
	Call ERASE_ALL;
	
	Call FINALIZE;
    
END_OPERATION;
