DATA:
    PATTERN  (--, --, FILE(--, --));      
    WREN   ( 1, 1, b01100000);
    WRDI   ( 1, 1, b00100000);
    RDSR   ( 1, 1, b10100000);
    READ   ( 1, 1, b11000000);
    FAST_READ   ( 1, 1, b11010000);
    PP   ( 1, 1, b01000000);
    BE   ( 1, 1, b11100011);
    SE   ( 1, 1, b00011011);
    DP   ( 1, 1, b10011101);
    RES   ( 1, 1, b11010101);  	
    DEVICE_ID  (1,1,FILE(ID, --)); 
DATA_END;

PROTOCOL:	PCM;

SIGNAL_DECL: 
	inout L_f_cs;
	buffer  clock;
SIGNAL_DECL_END;
             
BUS_DECL: 
    /CE = L_f_cs;
BUS_DECL_END;

FUNCTION INITIAL;
	ENDDR DRPAUSE;
	ENDIR IRPAUSE;
	clock   HIGH;
	SET_ROW (PATTERN, 0);
	Addr = START_ADDRESS;
	SIR	8	TDI(FF) ;
	RUNTEST	IDLE	32 TCK	2.00E-002 SEC;
	# Shift SPI_PROGRAM instruction 
	SIR	8	TDI(SPI_PROGRAM) ;
	RUNTEST	IDLE	32 TCK	2.00E-002 SEC;
END_FUNCTION;

FUNCTION FINALIZE;
	INT32 secure = 0;
	INT32 quad = 0;
	INT32 byteMode = 0;
	
	secure = _SECURE_;
	quad = _QUAD_;
	byteMode = _3BYTESMODE_;
	IF (secure Equal 1);
		Call SECURE;
	ELSE;
		secure = 0;
	END_IF;
	
	IF (quad Equal 1);
		Call QUAD;
	ELSE;
		quad = 0;
	END_IF;	
	
	IF (byteMode Equal 1);
		Call EXIT_3_BYTE_MODE;
	ELSE;
		byteMode = 0;
	END_IF;	
		
	# Shift BYPASS instruction (0xFF) 
	SIR	8	TDI(FF) ;
	RUNTEST	IDLE	32 TCK	2.00E-002 SEC;
END_FUNCTION;

FUNCTION EXIT_3_BYTE_MODE;
	& Exit 4-Byte mode...;
	SDR 16 TDI(00E8+TDR);
	DELAY 1.00E-001 SEC;
END_FUNCTION;

FUNCTION ERASE_ALL_FINALIZE;
	# Shift BYPASS instruction (0xFF) 
	SIR	8	TDI(FF) ;
	RUNTEST	IDLE	32 TCK	2.00E-002 SEC;
END_FUNCTION;

FUNCTION CHECK_ID;
	HDR 0;
	TDR 0;
	DOTRY 1;
	SDR 32 TDI(HDR+000000F9+TDR) TDO(DEVICE_ID+HDR+FFFFFF) MASK(DEVICE_ID+HDR+000000);
	ELSE;
	SDR 40 TDI(HDR+00000000D5+TDR) TDO(DEVICE_ID+HDR+FFFFFFFF) MASK(DEVICE_ID+HDR+00000000);
	ENDTRY;
	& Enter 4-byte mode...;
	SDR 16 TDI(01E8+TDR);
	DELAY 1.00E SEC;
END_FUNCTION;

FUNCTION CHECK_RDID;
	HDR 0;
	TDR 0;
	DOTRY 1;
	SDR 16 TDI(HDR+00F9+TDR) TDO(VENDOR_ID+HDR+FF) MASK(VENDOR_ID+HDR+00);
	ELSE;
	SDR 40 TDI(HDR+0000000009+TDR) TDO(VENDOR_ID+HDR+FFFFFFFF) MASK(VENDOR_ID+HDR+00000000);
	ENDTRY;
END_FUNCTION;

FUNCTION SECURE;
	INT32 SecureEnable = 0;
	SecureEnable = _SECURE_ENABLE_;
	& Secure Device...;
	Call ENABLE;
	IF (SecureEnable Equal 0);
		SDR 16 TDI(1880+TDR);
		LOOP 100;
		LDELAY  DRPAUSE 1.00E-001 SEC;
			SDR 16 TDI(HDR+00A0+TDR) TDO(h18+HDR+FF) MASK(hFF+HDR+00);
		END_LOOP;
	ELSE;
		SecureEnable = _SECURE_ENABLE_;
	END_IF;
	IF (SecureEnable Equal 1);
		SDR 16 TDI(3880+TDR);
		LOOP 100;
		LDELAY  DRPAUSE 1.00E-001 SEC;
			SDR 16 TDI(HDR+00A0+TDR) TDO(h1C+HDR+FF) MASK(hFF+HDR+00);
		END_LOOP;
	ELSE;
		SecureEnable = _SECURE_ENABLE_;
	END_IF;
	IF (SecureEnable Equal 2);
		SDR 16 TDI(0480+TDR);
		LOOP 100;
		LDELAY  DRPAUSE 1.00E-001 SEC;
			SDR 16 TDI(HDR+00A0+TDR) TDO(h20+HDR+FF) MASK(hFF+HDR+00);
		END_LOOP;
	ELSE;
		SecureEnable = _SECURE_ENABLE_;
	END_IF;
	IF (SecureEnable Equal 3);
		SDR 16 TDI(2480+TDR);
		LOOP 100;
		LDELAY  DRPAUSE 1.00E-001 SEC;
			SDR 16 TDI(HDR+00A0+TDR) TDO(h24+HDR+FF) MASK(hFF+HDR+00);
		END_LOOP;
	ELSE;
		SecureEnable = _SECURE_ENABLE_;
	END_IF;
	IF (SecureEnable Equal 4);
		SDR 16 TDI(1480+TDR);
		LOOP 100;
		LDELAY  DRPAUSE 1.00E-001 SEC;
			SDR 16 TDI(HDR+00A0+TDR) TDO(h28+HDR+FF) MASK(hFF+HDR+00);
		END_LOOP;
	ELSE;
		SecureEnable = _SECURE_ENABLE_;
	END_IF;
	Call DISABLE;
END_FUNCTION;

FUNCTION QUAD;
	INT32 QuadEnable = 0;
	QuadEnable = _QUAD_ENABLE_;
	& Program Quad Enable...;
	Call ENABLE;
	IF (QuadEnable Equal 0);
		SDR 16 TDI(0280+TDR);
	ELSE;
		QuadEnable = _QUAD_ENABLE_;
	END_IF;
	IF (QuadEnable Equal 1);
		SDR 24 TDI(000280+TDR);
	ELSE;
		QuadEnable = _QUAD_ENABLE_;
	END_IF;
	IF (QuadEnable Equal 2);
		SDR 32 TDI(0E600080+TDR);
	ELSE;
		QuadEnable = _QUAD_ENABLE_;
	END_IF;
	IF (QuadEnable Equal 3);
		SDR 24 TDI(400080+TDR);
	ELSE;
		QuadEnable = _QUAD_ENABLE_;
	END_IF;
	IF (QuadEnable Equal 4);
		SDR 16 TDI(408C+TDR);
	ELSE;
		QuadEnable = _QUAD_ENABLE_;
	END_IF;
	DELAY 10.00E-000 SEC;
	Call DISABLE;	
END_FUNCTION;

FUNCTION UNSECURE;
	& UnLock Device...;
	Call ENABLE;
	SDR 16 TDI(0080+TDR);
	DELAY 1.00E-001 SEC;
	LOOP 100;
	LDELAY  DRPAUSE 1.00E-001 SEC;
		SDR 16 TDI(HDR+00A0+TDR) TDO(h00+HDR+FF) MASK(h83+HDR+00);
	END_LOOP;
	Call DISABLE;
END_FUNCTION;


FUNCTION ENABLE;
	SDR 8 TDI(60+TDR);
	DELAY 1.00E-003 SEC;
END_FUNCTION;

FUNCTION ERASE_ALL;
	SDR 8 TDI(E3+TDR);
	DELAY 1.00E SEC;
	LOOP 200;
	LDELAY  DRPAUSE 1.00E SEC;
		SDR 16 TDI(HDR+00A0+TDR) TDO(h00+HDR+FF) MASK(h83+HDR+00);
	END_LOOP;
END_FUNCTION;
FUNCTION ERASE;
	INT32 AddrIdx = 0;
	INT32 EndAddrIdx = 0;
	Addr = START_ADDRESS;
	EndAddrIdx = END_ADDRESS;
	IF (Addr Equal 0);
		AddrIdx = 0;
	ELSE;
		AddrIdx = Addr/65536;
	END_IF;
	IF (EndAddrIdx Equal 0);
		EndAddrIdx = DEVICE_SIZE;
	ELSE;
		EndAddrIdx = EndAddrIdx/65536;
		EndAddrIdx = EndAddrIdx +1;
	END_IF;
	AddrIdx = EndAddrIdx - AddrIdx;	
	REPEAT AddrIdx;
		Call ENABLE;
		SDR 32 TDI(Addr+1B+TDR);
		LOOP 5000;
		LDELAY  DRPAUSE 1.00E-001 SEC;
		SDR 16 TDI(HDR+00A0+TDR) TDO(h00+HDR+FF) MASK(h83+HDR+00);
		END_LOOP;
		Addr = Addr+65536;
	END_REPEAT;

	
END_FUNCTION;
FUNCTION PROGRAM;
	Addr = START_ADDRESS;
	INT32 BuffAddr = 0;
	RESET_BURST;
	REPEAT 0;
		IF (PATTERN[BuffAddr...256] NotEqual hFF);
			Call ENABLE;
			SDR 32 TDI(PATTERN+Addr+40+TDR);
			LOOP 1000;
			LDELAY  DRPAUSE 1.00E-003 SEC;
				SDR 16 TDI(HDR+00A0+TDR) TDO(h00+HDR+FF) MASK(h83+HDR+00);
			END_LOOP;
			Addr = Addr +256;
			BuffAddr = BuffAddr +256;
		ELSE;
			Addr = Addr +256;
			BuffAddr = BuffAddr +256;
		END_IF;		
    END_REPEAT;
END_FUNCTION;

FUNCTION DISABLE;
	SDR 8 TDI(20+TDR);
	DELAY 1.00E-003 SEC;
END_FUNCTION;

FUNCTION VERIFY;
	Addr = START_ADDRESS;
	RESET_BURST;
	INT32 split = 0;
	split = _SPLIT_VERIFY_;
	IF (split Equal 1);
		Call SPLIT_VERIFY;
	ELSE;	
		SDR 40  TDI(HDR+00+Addr+D0+TDR) TDO(PATTERN+HDR+FFFFFFFFFFFF) MASK(PATTERN+HDR+000000000000);
	END_IF;
END_FUNCTION;

FUNCTION SPLIT_VERIFY;
	Addr = START_ADDRESS;
	RESET_BURST;
	REPEAT 0;
		Call ENABLE;
		SDR 40  TDI(HDR+00+Addr+D0+TDR) TDO(PATTERN[256]+HDR+FFFFFFFFFFFF) MASK(PATTERN[256]+HDR+000000000000);
		Addr = Addr +256;		
    END_REPEAT;
END_FUNCTION;

FUNCTION BLANK_CHECK;
	Addr = START_ADDRESS;
	RESET_BURST;
	SDR 40  TDI(HDR+00+Addr+D0+TDR) TDO(BLANK_PATTERN+HDR+FFFFFFFFFFFF) ;
END_FUNCTION;

FUNCTION READBACK;
	Addr = START_ADDRESS;
	RESET_BURST;
	SDR 40  TDI(HDR+00+Addr+D0+TDR) TDO(SAVE_PATTERN+HDR+FFFFFFFFFFFF) ;
END_FUNCTION;

FUNCTION UPLOAD;
	INT32 Idx1 = 0;
	SIR	8	TDI (23) ;
	RUNTEST	IDLE	5 TCK	2.00E-002 SEC;
	SIR	8	TDI (FF) ;
	RUNTEST	IDLE	5 TCK	2.00E-002 SEC;
	SIR	8	TDI (23) ;
	RUNTEST	IDLE	5 TCK	2.00E-002 SEC;
	SIR	8	TDI (FF) ;
	RUNTEST	IDLE	5 TCK	2.00E-002 SEC;
END_FUNCTION;

OPERATION Verify ID;
  	INT32 Addr = 0;
    Call INITIAL;
    Call CHECK_RDID;
    Call CHECK_ID;
END_OPERATION;

OPERATION Erase,Program,Verify;
  	INT32 Addr = 0;
    $ Initializing...
	Call INITIAL;
		
    $ IDCode Checking...
	Call CHECK_ID;
	
    $ Enabling...
	Call ENABLE;
    	
    $ Erasing...
	Call ERASE;

	$ Disabling...
	Call DISABLE;
	
	$ Enabling...
	Call ENABLE;
	
    $ Programming...
	Call PROGRAM;

    $ Disabling...
	Call DISABLE;

    $ Verifying...
	Call VERIFY;
	
	Call FINALIZE;
       
END_OPERATION;

