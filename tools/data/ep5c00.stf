<!-- *****************************  Bitgen *************************** -->
<!-- new: PROPUI_BIT_SearchPthBitGen -->
<PropertySet	
	name="PROPSET_BITGEN" >
  <Include>
      <PropertyUIItem> PROPUI_BIT_RunDRCBitGen </PropertyUIItem>
      <PropertyUIItem> PROPUI_BIT_CreateBitFile </PropertyUIItem>
